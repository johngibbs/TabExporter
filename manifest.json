{
  "manifest_version": 3,
  "name": "Tab Exporter",
  "version": "1.0",
  "description": "Export your browser tabs and groups to a markdown file",
  "permissions": [
    "tabs",
    "windows",
    "downloads"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
