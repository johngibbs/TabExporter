{
  "manifest_version": 3,
  "name": "Tab Exporter",
  "version": "1.0",
  "description": "Export your browser tabs and groups to a markdown file",
  "permissions": [
    "tabs",
    "windows",
    "downloads",
    "tabGroups",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
